.nav_l { display:none; }
.nav_r { display:none; }
.navigation { padding-left:0px; }
.navigation li { float:left; width:100px; text-align:center; position:relative; z-index:100; padding-right:0px;}
.navigation li a, .navigation li a:visited { display: block; text-decoration: none; color: #fff;}
.navigation li a:hover, .navigation li.hover a { color: #fff; text-decoration: none; background:url(../images/nav_1.gif) center top no-repeat; width:100px; height:35px;}
.navigation li.hover a:visited { _color:#fff; }
.navigation ul { display:none; }
.navigation ul li { background-image:none; background-color:#890a0b; line-height:35px; margin:0px; padding: 0px; height:35px; width:100px; }
.navigation ul li a, .navigation ul li a:hover { background-image:none; color:#fff; width:100px; margin:0px; padding: 0px; height:35px;}
.navigation li ul { width:100px; display:none; position:absolute; top:35px; left:9px;  margin: 0px; padding:0; z-index:100; background-color:#890a0b; line-height:27px;}
.navigation li ul ul { left:100px; top:50px; z-index:200; top:0; width:100px;  line-height:27px;}
.navigation li ul li { background-color:#004682; border:0; background-image:none; height:27px; width:100px;  line-height:27px;}
.navigation li ul li a { color:#fff; font-weight:normal; text-align:left; text-indent:10px; background-image:none; height:27px; width:100px;  line-height:27px;}
.navigation li ul li a:visited { _color:#fff; background-image:none; font-weight:normal; color:#fff; height:27px;  width:100px; line-height:27px;}
.navigation li ul li a:hover { color:#fff; background-image:none; background:#000; height:27px;  width:100px; line-height:27px;}
.navigation li ul li.hover { background:#000; background-image:none; width:100px; height:27px;color:#fff;  width:100px; line-height:27px;}
.navigation li.hover ul a { background-image:none; height:27px;  width:100px; line-height:27px;}




